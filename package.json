{
  "name": "remote-dev-team-hub",
  "version": "1.0.0",
  "description": "A real-time collaborative platform for distributed software engineering teams",
  "main": "setup.js",
  "scripts": {
    "setup": "node setup.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "start": "npm run start:backend",
    "start:backend": "cd backend && npm start",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "clean": "rm -rf node_modules backend/node_modules frontend/node_modules",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test"
  },
  "keywords": [
    "collaboration",
    "team-management",
    "kanban",
    "real-time",
    "socket.io",
    "react",
    "node.js",
    "postgresql",
    "firebase"
  ],
  "author": "Remote Dev Team Hub Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/remote-dev-team-hub.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/remote-dev-team-hub/issues"
  },
  "homepage": "https://github.com/your-username/remote-dev-team-hub#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
